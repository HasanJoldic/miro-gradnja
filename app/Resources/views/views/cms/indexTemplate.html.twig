{% macro startCmsElement(type, subtype, value) %}
<div class="cms-element" onClick="return selectMe('{{ type }}', '{{ subtype }}', '{{ value }}');" data-toggle="modal"
     data-target="modal">
    {% endmacro startCmsElement %}
    {% macro endCmsElement() %}
</div>
{% endmacro endCmsElement %}

{% macro navLink(subtype, href, innerText) %}
    {% import _self as forms %}
    {{ forms.startCmsElement("NAV_LINK", subtype, innerText) }}
    <li class="nav-item">
        <a class='nav-link admin-nav-link {% if path(app.request.attributes.get("_route"),
        app.request.attributes.get("_route_params")) == "/admin/" ~ href %}active{% endif %}'
           href="/admin/{{ href }}">{{ innerText }}</a>
    </li>
    {{ forms.endCmsElement() }}
{% endmacro navLink %}

