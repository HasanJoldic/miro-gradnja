{#web {% extends "views/web/base.html.twig"%} web#}
{#cms {% extends "views/cms/baseCms.html.twig"%} cms#}
{% block body %}
    {#cms {%  import "includes/includes/cms/macro.html.twig" as forms %} cms#}
    <div class="container index-container">
        <div class="row row-eq-height">
            <div class="col-lg-6 index-col">
                {#cms {{ forms.cmsElement() }} cms#}
                {#cms <div onClick='return selectMe("NAV_BRAND", "", "__insertFooterText__");'> cms#}
                <div class="index-jumbotron">
                    <a href="{#cms /cms cms#}/usluge/" class="">
                        <div class="jumbotron jumbotron-fluid">
                            <div class="container">
                                <h1 class="display-3">Nase usluge</h1><p class="lead">Pogledajte usluge koje nudimo*</p>
                                <p style="visibility: hidden; ">
                                    * Postoji mogucnost da list nije azurirana. Ako nismo naveli posao koji
                                        trazite, molimo da nas ipak kontaktirate
                                </p>
                                <p class="jumbotron-footer">
                                    * Postoji mogucnost da list nije azurirana. Ako nismo naveli posao koji
                                        trazite, molimo da nas ipak kontaktirate
                                </p>
                            </div>
                        </div>
                    </a>
                </div>
                {#cms </div> cms#}
                {#cms {{ forms.cmsElementEndTag() }} cms#}
            </div>
            <div class="col-lg-6 index-col">
                {#cms {{ forms.cmsElement() }} cms#}
                {#cms <div onClick='return selectMe("NAV_BRAND", "", "__insertFooterText__");'> cms#}
                <div class="index-jumbotron">
                    <a href="{#cms /cms cms#}/kontakt/" class="">
                        <div class="jumbotron jumbotron-fluid">
                            <div class="container">
                                <h1 class="display-3">Kontaktirajte nas</h1>
                                <p class="lead"><small>Dostupni smo putem telefona, emaila, kao i preko formulara kojeg
                                        nudimo na nasoj web stranici</small></p>
                            </div>
                        </div>
                    </a>
                </div>
                {#cms </div> cms#}
                {#cms {{ forms.cmsElementEndTag() }} cms#}
            </div>
        </div>
    </div>
{% endblock body %}